{
    "workbench.startupEditor": "newUntitledFile",
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "editor.renderWhitespace": "all",
    "editor.cursorStyle": "line",
    "editor.suggestSelection": "first",
    "editor.largeFileOptimizations": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.list.openMode": "doubleClick",
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "explorer.autoReveal": "focusNoScroll",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.project.importHint": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "hediet.vscode-drawio.local-storage": "",
    "redhat.telemetry.enabled": true,
    "yaml.customTags": [
        "!encrypted/pkcs1-oaep scalar",
        "!vault scalar",
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?",
    "editor.lineNumbers": "on",
    "[terraform]": {
        "editor.defaultFormatter": "hashicorp.terraform"
    },
    "[terraform-vars]": {
        "editor.defaultFormatter": "hashicorp.terraform"
    },
    "go.toolsManagement.autoUpdate": true,
    "yaml.schemas": {},
    "json.schemas": [],
    "quarkus.tools.alwaysShowWelcomePage": false,
    "editor.minimap.enabled": false,
    "workbench.iconTheme": "vscode-icons",
    "editor.suggest.showMethods": true,
    "editor.suggest.preview": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.snippetSuggestions": "top",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "yaml": true
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "gitlens.views.commits.files.layout": "list",
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "aws.codeWhisperer.shareCodeWhispererContentWithAWS": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "rust-analyzer.rustfmt.overrideCommand": null,
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "workbench.panel.defaultLocation": "right",
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.accessibilitySupport": "off",
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "dotnet.server.useOmnisharp": true,
    "[go]": {
        "editor.defaultFormatter": "golang.go"
    },
    "accessibility.signals.terminalBell": {
        "sound": "off"
    },
    "terminal.integrated.enableVisualBell": true,
    "accessibility.signals.lineHasError": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.lineHasWarning": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.lineHasFoldedArea": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.lineHasBreakpoint": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.lineHasInlineSuggestion": {
        "sound": "off"
    },
    "accessibility.signals.terminalQuickFix": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.onDebugBreak": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.noInlayHints": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.taskCompleted": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.taskFailed": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.terminalCommandFailed": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.notebookCellCompleted": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.notebookCellFailed": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.diffLineInserted": {
        "sound": "off"
    },
    "accessibility.signals.diffLineDeleted": {
        "sound": "off"
    },
    "accessibility.signals.diffLineModified": {
        "sound": "off"
    },
    "accessibility.signals.chatRequestSent": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.chatResponseReceived": {
        "sound": "off"
    },
    "accessibility.signals.clear": {
        "sound": "off",
        "announcement": "auto"
    },
    "hediet.vscode-drawio.resizeImages": null,
    "accessibility.signals.progress": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.positionHasError": {
        "sound": "off"
    },
    "accessibility.signals.positionHasWarning": {
        "sound": "off"
    },
    "editor.fontFamily": "'PlemolJP35',  'UDEV Gothic 35', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 14,
    "http.proxyStrictSSL": false,
    "makefile.configureOnOpen": true,
    "roo-cline.allowedCommands": [
        "npm test",
        "npm install",
        "tsc",
        "git log",
        "git diff",
        "git show",
        "pytest tests/integration/"
    ],
    "terminal.integrated.defaultProfile.osx": "zsh",
    "database-client.autoSync": true,
    "githubPullRequests.terminalLinksHandler": "github",
    "githubPullRequests.pullBranch": "never",
    "python.analysis.typeCheckingMode": "standard",
    "terminal.integrated.scrollback": 100000,
    "githubPullRequests.fileListLayout": "flat",
    "git.blame.editorDecoration.enabled": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.advanced": {},
    "github-actions.workflows.pinned.workflows": [],
    "docker.extension.enableComposeLanguageServer": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "editor.formatOnSave": true,
    "chat.tools.autoApprove": true,
    "chat.agent.maxRequests": 50,
}
